{
  "ReverseProxy": {
    "Routes": {
      "users": {
        "ClusterId": "userCluster",
        "Match": {
          "Path": "/users/{**catch-all}"
        }
      },
      "orders": {
        "ClusterId": "orderCluster",
        "Match": {
          "Path": "/orders/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "userCluster": {
        "Destinations": {
          "userApi": {
            "Address": "https://demogatewayusers.onrender.com/"
          }
        }
      },
      "orderCluster": {
        "Destinations": {
          "orderApi": {
            "Address": "https://demogatewayorders.onrender.com/"
          }
        }
      }
    }
  }
}
